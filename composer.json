{
    "name": "smurf/symfony2-app-bundle",
    "type": "symfony-bundle",
    "description": "Starter kit for Symfony2 web application",
    "keywords": ["web","application","bundle"],
    "license": "Creative Commons BY-NC-ND 4.0",
    "homepage": "http://www.blacksmurf.net",
    "authors": [
        {
            "name": "Julien Buonocore",
            "email": "julien.buonocore@gmail.com"
        }
    ],
    "require": {
        "php":                              ">=5.3.2",
        "symfony/framework-bundle":         ">=2.5",
        "twig/twig":                        "*",
        "doctrine/doctrine-bundle":         "*"        
    },
    "autoload": {
        "psr-0": { "Smurf\\Symfony2AppBundle": "" }
    },
    "target-dir": "Smurf/Symfony2AppBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }    
}
